      <constructor name="new" c:identifier="atspi_state_set_new">
        <doc xml:whitespace="preserve">Generates an #AtspiStateSet with the given @states.</doc>
        <return-value transfer-ownership="full">
          <doc xml:whitespace="preserve">A new #AtspiStateSet with the given states.</doc>
          <type name="StateSet" c:type="AtspiStateSet*"/>
        </return-value>
        <parameters>
          <parameter name="states" transfer-ownership="none">
            <doc xml:whitespace="preserve">An array of states with which the method initializes the state set.</doc>
            <array name="GLib.Array" c:type="GArray*">
              <type name="StateType"/>
            </array>
          </parameter>
        </parameters>
