      <parameters>
        <parameter name="role" transfer-ownership="none">
          <doc xml:whitespace="preserve">an #AtspiRole object to query.</doc>
          <type name="Role" c:type="AtspiRole"/>
        </parameter>
      </parameters>
    </function>
